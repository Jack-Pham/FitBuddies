<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0099cc"
    tools:context="com.example.juliethjaramillo.fitbuddies.home">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->

    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:gridSpec="0|8|#0093eeff|K:#ee8700ff:16,l:72,l:16,r|S:#83ee00ff:16,0,l:16,56,l:16,0,r"
        android:paddingRight="4dp"
        android:background="#ffbbdefb">
        <ImageButton
            android:layout_width="96.5dp"
            android:layout_height="69dp"
            android:id="@+id/imageButton1"
            android:background="@drawable/femaleavatar"
            android:layout_marginLeft="45dp"
            android:fadingEdge="horizontal"
            android:layout_marginTop="20dp" />
        <ImageButton
            android:layout_alignTop="@id/imageButton1"
            android:layout_width="96.5dp"
            android:layout_height="69.0dp"
            android:id="@+id/imageButton2"
            android:layout_gravity="right"
            android:layout_alignRight="@id/imageView3"
            android:background="@drawable/avatarmale"
            android:adjustViewBounds="true"
            />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="16dp"
            android:id="@+id/imageView5"
            android:background="@drawable/gm"
            android:layout_marginLeft="115dp"
            android:layout_gravity="left"

            android:layout_below="@id/imageButton1"
            />
        <ImageView
            android:layout_width="30dp"
            android:layout_height="16dp"
            android:id="@+id/imageView3"
            android:background="@drawable/us"

            android:layout_marginLeft="315dp"
            android:layout_below="@id/imageButton2"
            />

        <TextView
            android:text="Julieth Jaramillo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nameu"
            android:layout_below="@id/imageView5"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:textAlignment="center"
            android:layout_marginTop="0dp"
            android:textColor="#ff160a0a" />

        <TextView
            android:text="Jake Gyllenhall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nameb"
            android:textAlignment="center"
            android:layout_gravity="right"
            android:layout_marginRight="35dp"
            android:layout_marginTop="0dp"
            android:layout_alignBottom="@id/nameu"
            android:layout_alignParentRight="true"
            android:textColor="#ff190707" />

        <TextView
            android:text="Age"
            android:layout_width="41.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/ageuser"
            android:layout_marginLeft="35dp"
            android:textColor="#ff180808"
            android:layout_below="@id/nameu"
            />
        <TextView
            android:text="19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/au"
            android:layout_marginLeft="10dp"
            android:textColor="#ff0c0000"
            android:layout_toRightOf="@id/ageuser"
            android:layout_alignBottom="@id/ageuser"/>
        <TextView
            android:text="Age"
            android:layout_width="41.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/agebud"
            android:layout_marginRight="35dp"
            android:layout_below="@id/nameb"
            android:layout_alignParentRight="true"
            android:textAlignment="textEnd"
            android:textColor="#ff150707" />
        <TextView
            android:text="22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ab"
            android:layout_alignBottom="@id/ageuser"
            android:layout_toLeftOf="@id/agebud"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:textColor="#ff1b1111" />
        <TextView
            android:text="Points"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_below="@id/ageuser"
            android:layout_width="41.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:textColor="#ff1b1111"
            android:layout_marginLeft="35dp" />
        <TextView
            android:text="Points"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="41.5dp"
            android:layout_below="@id/agebud"
            android:layout_alignBottom="@id/textView11"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            android:textColor="#ff1b1111"
            android:layout_alignParentRight="true"
            android:textAlignment="textEnd"
            android:layout_marginRight="35dp" />
        <TextView
            android:text="250"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/textView11"
            android:id="@+id/pu"
            android:layout_toRightOf="@id/textView11"
            android:layout_marginLeft="10dp"
            android:textColor="#ff1b1111" />
        <TextView
            android:text="500"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:id="@+id/pb"
            android:layout_marginRight="10dp"
            android:textAlignment="textEnd"
            android:layout_alignBottom="@id/textView11"
            android:layout_toLeftOf="@id/textView12"
            android:textColor="#ff1b1111" />
        <CheckedTextView
            android:layout_below="@id/textView11"
            android:layout_width="80.5dp"
            android:layout_height="28.0dp"
            android:id="@+id/checkedTextView1"
            android:text="Activity"
            android:layout_marginRight="0.0dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="42dp"
            android:textSize="20dp"
            android:textColor="#ff1b1111" />
        <CheckedTextView
            android:layout_below="@id/textView11"
            android:layout_width="80.5dp"
            android:layout_height="28.0dp"
            android:id="@+id/checkedTextView2"
            android:text="Activity"
            android:textAlignment="textEnd"
            android:layout_marginRight="35.0dp"
            android:layout_marginTop="42dp"
            android:layout_alignParentRight="true"
            android:textSize="20dp"
            android:textColor="#ff1b1111" />
        <TextView
            android:text="Running"
            android:layout_width="144.0dp"
            android:layout_height="32.0dp"
            android:id="@+id/actu"
            android:layout_below="@id/checkedTextView1"
            android:layout_marginLeft="35dp"
            android:textColor="#ff1b1111"
            android:layout_marginTop="10dp" />
        <TextView
            android:text="Running"
            android:layout_width="144dp"
            android:layout_height="32dp"
            android:id="@+id/actb"
            android:layout_below="@id/checkedTextView1"
            android:layout_marginRight="35dp"
            android:layout_alignParentRight="true"
            android:textAlignment="textEnd"
            android:layout_marginTop="10dp"
            android:textColor="#ff1b1111" />
        <TextView
            android:text="30min"
            android:layout_below="@id/actu"
            android:layout_width="105.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/minu"
            android:layout_marginLeft="35dp"
            android:textColor="#ff1b1111" />
        <TextView
            android:text="30min"
            android:layout_width="105.5dp"
            android:layout_below="@id/actu"
            android:layout_height="wrap_content"
            android:id="@+id/minb"
            android:layout_marginRight="35.0dp"
            android:layout_alignParentRight="true"
            android:textAlignment="textEnd"
            android:textColor="#ff1b1111" />
        <TextView
            android:text="Calories Burned"
            android:layout_below="@id/minu"
            android:layout_width="121.0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textColor="#ff281515"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="35dp" />
        <TextView
            android:text="Calories Burned"
            android:layout_below="@id/minu"
            android:layout_width="121.0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:textColor="#ff281515"
            android:layout_marginRight="35dp"
            android:layout_marginTop="20dp"
            android:textAlignment="textEnd"
            android:layout_alignParentRight="true" />
        <TextView
            android:text="150 kcal"
            android:layout_width="78.0dp"
            android:layout_height="wrap_content"
            android:id="@+id/calb"
            android:layout_marginLeft="35dp"
            android:textColor="#ff240c0c"
            android:layout_below="@id/textView5"
            />
        <TextView
            android:text="200 kcal"
            android:layout_width="78.0dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:id="@+id/calu"
            android:textColor="#ff1c0404"
            android:layout_marginRight="35dp"
            android:textAlignment="textEnd"
            android:layout_below="@id/textView6"/>
        <TextView
            android:text="Total Points"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:textColor="#ff1c0c0c"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="20dp"
            android:layout_alignBottom="@id/textView8"
            android:layout_below="@id/calu" />
        <TextView
            android:text="Total Points"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:textColor="#ff201010"
            android:layout_width="115dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="35dp"
            android:textAlignment="textEnd"
            android:layout_alignParentRight="true"
            android:layout_below="@id/calu"/>
        <TextView
            android:text="100"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_height="wrap_content"
            android:id="@+id/vu"
            android:layout_width="match_parent"
            android:layout_alignBottom="@id/textView8"
            android:textColor="#ff1c0c0c"
            android:textAlignment="center" />
        <TextView
            android:text="100"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:id="@+id/vb"
            android:textColor="#ff240808"
            android:layout_marginTop="-27dp"
            android:layout_marginLeft="250dp" />
        <TextView
            android:text="Enjoy your workout!"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView13"
            android:textAlignment="center"
            android:paddingBottom="15dp"
            android:layout_above="@id/button1"
            android:textColor="#ff1d1818" />

        <Button
            android:text="Start Workout"
            android:layout_width="match_parent"
            android:layout_height="65.5dp"
            android:id="@+id/button1"
            android:background="#ff1e88e5"
            android:textSize="30dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

</FrameLayout>
